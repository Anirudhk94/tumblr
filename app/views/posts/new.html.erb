<div id="page_wrapper">
	<h1 id="content">Make something</h1>

	<%= form_for :post, url: posts_path do |f| %>
	<div id="content">
	  <p>
	  	<%= f.label :title %><br/>
	  	<%= f.text_field :title %>
	  </p>

	  <p>
	  	<%= f.label :body %><br/>
	  	<%= f.text_area :body %>
	  </p>
	 
	  <%= f.submit %>
	</div>  
	<% end %>
</div>